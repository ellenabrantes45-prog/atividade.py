salário = float(input("Digite o valor do salário por hora trabalhada"))
horas= float(input("Digite a quantidade de horas trabalhadas"))
saláriobruto = (salário*horas)
print ("Salário bruto: R$", saláriobruto)
ir = (saláriobruto*0.11)
print ("O valor do IR é R$:",ir)
inss= (saláriobruto*0.08) 
print("O valor do INSS é R$:",inss)
sindicato = (saláriobruto*0.05)
print("O valor do Sindicato é R$:", sindicato)
saláriolíquido = (saláriobruto - (ir+inss+sindicato))
print("Salário líquido: R$", saláriolíquido)